{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "cross-env ANT_DESIGN_PRO_ONLY_DO_NOT_USE_IN_YOUR_PRODUCTION=site npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks",
    "format-imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "set PORT=8101&&umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "^0.0.1-rc.1",
    "@ant-design/pro-layout": "^4.5.12",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.8-beta.1",
    "antd": "^4.0.0-rc.5",
    "bizcharts": "^3.5.5",
    "bizcharts-plugin-slider": "^2.1.1",
    "braft-editor": "^2.3.7",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.2.0",
    "dva": "^2.4.1",
    "enquire-js": "^0.2.1",
    "hash.js": "^1.1.7",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "memoize-one": "^5.0.5",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.0.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "queue": "^6.0.1",
    "react": "^16.9.0",
    "react-amap": "^1.2.8",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.9.0",
    "react-fittext": "^1.0.0",
    "react-infinite-scroller": "^1.2.4",
    "react-media": "^1.9.2",
    "react-media-hook2": "^1.1.2",
    "react-router-dom": "^5.0.1",
    "react-zmage": "^0.8.4",
    "redux": "^4.0.4",
    "umi": "^2.8.17",
    "umi-plugin-pro-block": "^1.3.4",
    "umi-plugin-react": "^1.9.15",
    "umi-request": "^1.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@ant-design/colors": "^3.1.0",
    "@ant-design/pro-cli": "^1.0.9",
    "@types/classnames": "^2.2.9",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.136",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.1",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.5",
    "@umijs/fabric": "^1.1.9",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.1.1",
    "enzyme": "^3.10.0",
    "eslint": "^6.1.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.3",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.3.0",
    "lint-staged": "^9.2.1",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "pro-download": "1.0.1",
    "slash2": "^2.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "umi-plugin-ga": "^1.1.5",
    "umi-plugin-pro": "^1.0.2",
    "umi-types": "^0.3.16",
    "webpack-theme-color-replacer": "^1.2.17"
  },
  "optionalDependencies": {
    "puppeteer": "^1.19.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
