{
  "name": "ant-design-pro",
  "version": "4.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "postinstall": "umi g tmp",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "docker-hub:build": "docker build  -f Dockerfile.hub -t  ant-design-pro ./",
    "docker-prod:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker-prod:dev": "docker-compose -f ./docker/docker-compose.yml up",
    "docker:build": "docker-compose -f ./docker/docker-compose.dev.yml build",
    "docker:dev": "docker-compose -f ./docker/docker-compose.dev.yml up",
    "docker:push": "npm run docker-hub:build && npm run docker:tag && docker push antdesign/ant-design-pro",
    "docker:tag": "docker tag ant-design-pro antdesign/ant-design-pro",
    "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "site": "npm run fetch:blocks && npm run build",
    "start": "cross-env PORT=8101 umi dev",
    "dev": "npm run start:dev",
    "start:dev": "cross-env PORT=8101 REACT_APP_ENV=dev MOCK=none umi dev",
    "start:dev-no-ui": "cross-env PORT=8101 REACT_APP_ENV=dev MOCK=none UMI_UI=none umi dev",
    "start:no-mock": "cross-env PORT=8101 MOCK=none umi dev",
    "start:no-ui": "cross-env PORT=8101 UMI_UI=none umi dev",
    "start:pre": "cross-env PORT=8101 REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env PORT=8101 REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@ant-design/pro-layout": "^5.0.17",
    "@ant-design/pro-table": "^2.4.0",
    "@antv/data-set": "^0.11.4",
    "@antv/l7": "^2.2.15",
    "@antv/l7-maps": "^2.2.15",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "antd": "^4.3.5",
    "bizcharts": "^4.0.6",
    "braft-editor": "^2.3.9",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "enquire-js": "^0.2.1",
    "gg-editor": "^3.1.2",
    "hash.js": "^1.1.7",
    "html-react-parser": "^0.13.0",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "memoize-one": "^5.1.1",
    "moment": "^2.27.0",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^6.1.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "queue": "^6.0.1",
    "rc-animate": "^3.1.0",
    "rc-banner-anim": "^2.4.4",
    "rc-queue-anim": "^1.8.5",
    "rc-scroll-anim": "^2.7.4",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^2.7.2",
    "react": "^16.13.1",
    "react-amap": "^1.2.8",
    "react-bmap": "^1.0.117",
    "react-container-query": "^0.11.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.6",
    "react-infinite-scroller": "^1.2.4",
    "react-media": "^1.10.0",
    "react-media-hook2": "^1.1.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^12.2.1",
    "react-zmage": "^0.8.5",
    "redux": "^4.0.5",
    "umi": "^3.2.4",
    "umi-request": "^1.3.5",
    "use-merge-value": "^1.0.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@ant-design/colors": "^4.0.1",
    "@ant-design/pro-cli": "^2.0.2",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.6",
    "@types/history": "^4.7.6",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.156",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.38",
    "@types/react-document-title": "^2.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@umijs/fabric": "^2.1.1",
    "@umijs/plugin-blocks": "^2.1.4",
    "@umijs/preset-ant-design-pro": "^1.2.2",
    "@umijs/preset-react": "^1.5.10",
    "@umijs/preset-ui": "^2.1.15",
    "carlo": "^0.9.46",
    "chalk": "^4.1.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^7.0.2",
    "cross-port-killer": "^1.2.1",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "7.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^23.16.0",
    "eslint-plugin-unicorn": "^20.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.2.11",
    "mockjs": "^1.1.0",
    "node-fetch": "^2.6.0",
    "npm-check-updates": "^7.0.1",
    "prettier": "^2.0.5",
    "pro-download": "1.0.1",
    "puppeteer-core": "^4.0.0",
    "slash2": "^2.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "optionalDependencies": {
    "puppeteer": "^4.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
